@model IEnumerable<WebECP.Models.Voter>

@{
    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>VoterDetails</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cnic)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NationalAssembly)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProvincialAssembly)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecretCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NationalVote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProvincialVote)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cnic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NationalAssembly)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProvincialAssembly)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecretCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NationalVote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProvincialVote)
            </td>
           
        </tr>
    }
    
    </table>
</body>
</html>
