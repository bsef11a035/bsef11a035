@model IEnumerable<WebECP.Models.NationalAssembly>

@{
    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NAResult</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NationalAssemblyNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CandidateCnic)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VotesNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Winner)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NationalAssemblyNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateCnic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VotesNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Winner)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
